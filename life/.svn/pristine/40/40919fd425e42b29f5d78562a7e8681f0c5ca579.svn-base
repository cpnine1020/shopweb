package com.xiangxiaole.www.model;

import java.io.Serializable;
import java.util.Date;

import com.xiangxiaole.util.tool.Formatter;

public class HouseSign implements Serializable {
    private Integer id;

    private Date signDate;

    private Integer webSignNum;

    private Float webSignSize;

    private Integer houseSignNum;

    private Float houseSignSize;

    private Integer webCheckNum;

    private Float webCheckSize;

    private Integer houseCheckNum;

    private Float houseCheckSize;

    private Date updateTime;

    private String city;

    public Integer getId() {
        return id;
    }

    public void setId(Integer id) {
        this.id = id;
    }

    public String getSignDate() {
        return Formatter.DateToStr(signDate, "yyyy-MM-dd");
    }

    public void setSignDate(Date signDate) {
        this.signDate = signDate;
    }

    public Integer getWebSignNum() {
        return webSignNum;
    }

    public void setWebSignNum(Integer webSignNum) {
        this.webSignNum = webSignNum;
    }

    public Float getWebSignSize() {
        return webSignSize;
    }

    public void setWebSignSize(Float webSignSize) {
        this.webSignSize = webSignSize;
    }

    public Integer getHouseSignNum() {
        return houseSignNum;
    }

    public void setHouseSignNum(Integer houseSignNum) {
        this.houseSignNum = houseSignNum;
    }

    public Float getHouseSignSize() {
        return houseSignSize;
    }

    public void setHouseSignSize(Float houseSignSize) {
        this.houseSignSize = houseSignSize;
    }

    public Integer getWebCheckNum() {
        return webCheckNum;
    }

    public void setWebCheckNum(Integer webCheckNum) {
        this.webCheckNum = webCheckNum;
    }

    public Float getWebCheckSize() {
        return webCheckSize;
    }

    public void setWebCheckSize(Float webCheckSize) {
        this.webCheckSize = webCheckSize;
    }

    public Integer getHouseCheckNum() {
        return houseCheckNum;
    }

    public void setHouseCheckNum(Integer houseCheckNum) {
        this.houseCheckNum = houseCheckNum;
    }

    public Float getHouseCheckSize() {
        return houseCheckSize;
    }

    public void setHouseCheckSize(Float houseCheckSize) {
        this.houseCheckSize = houseCheckSize;
    }

    public String getUpdateTime() {
    	return Formatter.DateToStr(updateTime, "yyyy-MM-dd HH:mm:ss");
    }

    public void setUpdateTime(Date updateTime) {
        this.updateTime = updateTime;
    }

    public String getCity() {
        return city;
    }

    public void setCity(String city) {
        this.city = city;
    }
}